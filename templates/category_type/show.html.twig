<!DOCTYPE html>

<title>Category</title>

{% block body %}
    <h1>Category</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ category.title }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ category.content }}</td>
            </tr>
            <tr>
                <th>Published</th>
                <td>{{ category.published ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('category_type_index') }}">back to list</a>

    <a href="{{ path('category_type_edit', {'id': category.id}) }}">edit</a>

    {{ include('category_type/_delete_form.html.twig') }}
{% endblock %}
